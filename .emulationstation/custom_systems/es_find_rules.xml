<?xml version="1.0"?>
<!-- This is the ES-DE find rules configuration file for Unix -->
<ruleList>
    <emulator name="OS-SHELL">
        <!-- Operating system shell -->
        <rule type="systempath">
            <entry>bash</entry>
            <entry>sh</entry>
        </rule>
    </emulator>
    <emulator name="RETROARCH_MOUNT_FACADE">
        <rule type="staticpath">
            <!-- Use facades for emulators to be able to mount instead of downloading ROMs. -->
            <entry>~/facades/emulator-launcher.sh</entry>
        </rule>
    </emulator>
    <emulator name="RETROARCH">
        <rule type="staticpath">
            <!-- Use facades for RetroArch to be able to specify a custom config path. -->
            <entry>~/facades/start-retroarch.sh</entry>
        </rule>
    </emulator>
    <core name="RETROARCH">
        <rule type="corepath">
            <entry>~/.retroarch/cores</entry>
        </rule>
    </core>
    <!--
        ################################################################
        ##### Facades for various emulators without libretro core. #####
        ################################################################
    -->
    <emulator name="FACADE_DUCKSTATION_SETUP">
        <rule type="staticpath">
            <entry>~/facades/duckstation/start-setup.sh</entry>
        </rule>
    </emulator>
    <emulator name="FACADE_PCSX2_SETUP">
